              #
            } elsif ($ns == MATHMLNS and $xmlns eq 'http://www.w3.org/1998/Math/MathML') {
              #
            } else {
              push @$Errors, {type => 'foreign:bad xmlns value',
                              level => 'm',
                              value => $xmlns,
                              di => $token->{attrs}->{xmlns}->{di},
                              index => $token->{attrs}->{xmlns}->{index}};
