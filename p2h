  if ($EOF) {
  
            push @$Tokens, {type => END_OF_FILE_TOKEN, tn => 0,
                            di => $DI,
                            index => $Offset + pos $Input};
          
  return 1;
  } else {
  return 1;
  }
  }
  return 0;
  };
  $StateActions->[PLAINTEXT_STATE_CR] = sub {
  if ($Input =~ /\G([\
  ])/gcs) {
  $State = PLAINTEXT_STATE;
  } elsif ($Input =~ /\G([\])/gcs) {
  
            push @$Tokens, {type => TEXT_TOKEN, tn => 0,
                            value => q@
