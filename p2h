          ## [154] in head noscript;START:head,noscript
          sub {
            my $token = $_;
  push @$Errors, {type => 'in noscript',
                                              level => 'm',
                                              di => $token->{di},
                                  index => $token->{index}};
  
            if ($token->{self_closing_flag}) {
              push @$Errors, {type => 'nestc',
