  } else {
  return 1;
  }
  }
  return 0;
  };
  $StateActions->[RCDATA_STATE___CHARREF_STATE_CR] = sub {
  if ($Input =~ /\G([\
  ])/gcs) {
  $State = RCDATA_STATE___CHARREF_STATE;
  } elsif ($Input =~ /\G([\])/gcs) {
  
            push @$Tokens, {type => TEXT_TOKEN, tn => 0,
                            value => $Temp,
                            di => $DI,
