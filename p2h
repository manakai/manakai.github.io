                last FOSTER;
              } elsif ($OE->[$i]->{et} & (TEM_ELS)) { # template
                push @$OP, ['text', [[$token->{value}, $token->{di}, $token->{index}]] => $OE->[$i]->{id}];
                last FOSTER;
              }
            }
            push @$OP, ['text', [[$token->{value}, $token->{di}, $token->{index}]] => $OE->[0]->{id}];
