  $StateActions->[COMMENT_END_STATE] = sub {
  if ($Input =~ /\G([\])/gcs) {
  push @{$Token->{q<data>}}, [q@--@, $DI, $Offset + (pos $Input) - (length $1) - 2];
  push @{$Token->{q<data>}}, [q@
  @, $DI, $Offset + (pos $Input) - (length $1) - 0];
