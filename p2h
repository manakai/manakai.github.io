  $HEAD_ELEMENT = $node_head;
  
            $IM = IN_HEAD_IM;
            #warn "Insertion mode changed to |in head| ($IM)";
          
  push @$OP, ['popped', [pop @$OE]];
  
            $IM = AFTER_HEAD_IM;
            #warn "Insertion mode changed to |after head| ($IM)";
          
  
