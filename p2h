  push @{$Token->{q<data>}}, [$3, $DI, $Offset + $-[3]];
  $State = COMMENT_END_BANG_STATE;
  push @{$Token->{q<data>}}, [q@--!@, $DI, $Offset + (pos $Input) - (length $1) - 3];
  $State = COMMENT_END_DASH_STATE;
  } elsif ($Input =~ /\G\!(\-)\-\-([^\␀\\-\<\>])([^\␀\\-\<]*)\-\-\>/gcs) {
  
