  package Web::DOM::HTMLTableSectionElement;
  our $VERSION = '1.0';
  push our @ISA, qw(Web::DOM::HTMLElement);
  use Web::DOM::Internal;
  use Web::DOM::Node;
  use Web::DOM::Element;
  
