          ## [127] in column group;START-ELSE
          sub {
            my $token = $_;
  
            if (not ($OE->[-1]->{et} & (COL_ELS))) {
              push @$Errors, {type => 'in colgroup',
                                              level => 'm',
                                              di => $token->{di},
                                  index => $token->{index}};
  
            if ($token->{self_closing_flag}) {
              push @$Errors, {type => 'nestc',
                                              level => 'm',
                                              text => $token->{tag_name},di => $token->{di},
                                  index => $token->{index}};
            }
          
