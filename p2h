                                              di => $token->{di},
                                  index => $token->{index}};
  &reconstruct_afe_foster if @$AFE and ref $AFE->[-1];
  
          my $node = {id => $NEXT_ID++,
                   token => $token,
                   di => $token->{di}, index => $token->{index},
                   ns => HTMLNS,
