            if (grep { $_->{value} =~ /[^\x09\x0A\x0C\x20]/ } @$TABLE_CHARS) {
              push @$Errors, {type => 'in table:#text',
                                              level => 'm',
                                              di => $TABLE_CHARS->[0]->{di},
                          index => $TABLE_CHARS->[0]->{index}};
  
            for my $token (@$TABLE_CHARS) {
              
