          FOSTER: {
            for my $i (reverse 1..$#$OE) {
              if ($OE->[$i]->{et} & (TAB_ELS)) { # table
                push @$OP, ['insert-foster', $node => $OE->[$i]->{id}, $OE->[$i-1]->{id}];
                last FOSTER;
