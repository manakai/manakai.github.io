        }
      ) {
              pop @$OE while $OE->[-1]->{et} & (DD_ELS | DT_ELS | LI_ELS | OPT_OPT_ELS | P_ELS | RB_RP_RT_ELS | RTC_ELS);
  
            if (not ($OE->[-1]->{et} & (RUB_ELS))) {
