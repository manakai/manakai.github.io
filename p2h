            if (not defined $InForeign) {
              pos ($Input) -= length $1;
              return 1;
            } else {
              if ($InForeign) {
                $State = CDATA_SECTION_STATE;
                return 0;
              }
            }
          
  
              push @$Errors, {type => 'bogus comment', level => 'm',
                              di => $DI, index => $Offset + (pos $Input) - 1};
            
  
          $Token = {type => COMMENT_TOKEN, tn => 0, 
