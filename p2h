  my $TokenizerAbortingTagNames = {
    title => 1,
    textarea => 1,
    plaintext => 1,
    style => 1,
    script => 1,
    xmp => 1,
    iframe => 1,
    noembed => 1,
    noframes => 1,
    noscript => 1,
  
    #html => 1, # for <html manifest> -> see the line with "first start tag"
    #meta => 1, # for <meta charset>
  };
    
  
  
  sub strict_checker ($;$) {
    if (@_ > 1) {
      $_[0]->{strict_checker} = $_[1];
    }
    return $_[0]->{strict_checker} || 'Web::XML::Parser::MinimumChecker';
  } # strict_checker
  
    
