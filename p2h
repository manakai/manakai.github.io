          for my $attr (@{$token->{attr_list} or []}) {
            $attr->{name_args} = $map->{$attr->{name}} || [undef, [undef, $attr->{name}]];
          }
        
  
        push @$OP, ['insert', $node => $OE->[-1]->{id}];
      
