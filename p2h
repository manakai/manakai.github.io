                                              level => 'm',
                                              di => $token->{di},
                                  index => $token->{index} + $-[1]};
              }
              
            
                    $token->{index} += length $1;
                  }
                }
              } else {
                &reconstruct_afe_foster if @$AFE and ref $AFE->[-1];
  
        if ($OE->[-1]->{et} & (TAB_ELS | TBO_TFO_THE_ELS | TR_ELS)) { # table* context
          FOSTER: {
            for my $i (reverse 1..$#$OE) {
              if ($OE->[$i]->{et} & (TAB_ELS)) { # table
