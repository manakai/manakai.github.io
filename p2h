  push our @ISA, qw(Web::DOM::HTMLElement);
  use Web::DOM::Element;
  
  _define_reflect_unsigned_long_positive_fb span => 'span', sub { 1 };
  
  _define_reflect_string align => 'align';
  _define_reflect_string width => 'width';
