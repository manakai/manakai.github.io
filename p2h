                last FOSTER;
              } elsif ($OE->[$i]->{et} & (TEM_ELS)) { # template
                push @$OP, ['insert', $node => $OE->[$i]->{id}];
                last FOSTER;
              }
            }
            push @$OP, ['insert', $node => $OE->[0]->{id}];
          } # FOSTER
        } else {
          push @$OP, ['insert', $node => $OE->[-1]->{id}];
        }
      
  
  push @$OE, $node;
  
            if ($token->{self_closing_flag}) {
