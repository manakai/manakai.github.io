  $Temp .= $1;
  $State = ATTR_VALUE__SQ__STATE___CHARREF_HEX_NUM_STATE;
  } elsif ($Input =~ /\G([\␀])/gcs) {
  
              push @$Errors, {type => 'bare hcro', level => 'm',
