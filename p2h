        ,
          ## [124] in column group;END-ELSE
          sub {
            my $token = $_;
  
            if (not ($OE->[-1]->{et} & (COL_ELS))) {
              push @$Errors, {type => 'in colgroup',
                                              level => 'm',
                                              di => $token->{di},
                                  index => $token->{index}};
  return;
            } else {
              push @$OP, ['popped', [pop @$OE]];
  
            $IM = IN_TABLE_IM;
            #warn "Insertion mode changed to |in table| ($IM)";
          
  
          goto &{$ProcessIM->[$IM]->[$token->{type}]->[$token->{tn}]};
        
