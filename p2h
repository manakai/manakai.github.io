            }
          
  
            ## Adjusted current node
            my $ns = ((defined $CONTEXT and @$OE == 1) ? $CONTEXT : $OE->[-1])->{ns};
          
  
          my $node = {id => $NEXT_ID++,
                      token => $token,
                      di => $token->{di}, index => $token->{index},
                      ns => $ns,
                      local_name => $token->{tag_name},
                      attr_list => $token->{attr_list},
