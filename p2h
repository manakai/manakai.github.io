            push @popped, pop @$OE;
            push @$OP, ['popped', \@popped];
          }
  
          pop @$AFE while ref $AFE->[-1];
          pop @$AFE; # #marker
        
  
            $IM = IN_TABLE_IM;
            #warn "Insertion mode changed to |in table| ($IM)";
          
            }
          
