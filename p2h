  package Web::DOM::HTMLTableElement;
  our $VERSION = '2.0';
  push our @ISA, qw(Web::DOM::HTMLElement);
  use Web::DOM::Internal;
  use Web::DOM::Node;
  use Web::DOM::Element;
  
