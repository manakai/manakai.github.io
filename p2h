                                                     },
                                         "name" => "Big5",
                                         "output" => "big5"
                                       },
                             "euc-jp" => {
                                           "ascii_compat" => 1,
                                           "compat_name" => "EUC-JP",
                                           "html_conformance" => "avoid",
                                           "html_decl_mapped" => "euc-jp",
                                           "key" => "euc-jp",
                                           "labels" => {
                                                         "cseucpkdfmtjapanese" => {},
                                                         "euc-jp" => {},
