  sub list ($) {
    my $id = $_[0]->get_attribute_ns (undef, 'list');
    return undef unless defined $id;
    my $el = $_[0]->owner_document->get_element_by_id ($id) or return undef;
    return $el if $el->manakai_element_type_match (HTML_NS, 'datalist');
    return undef;
  } # list
  
