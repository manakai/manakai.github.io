          ## [166] in head;START:meta
          sub {
            my $token = $_;
  
          my $node = {id => $NEXT_ID++,
                   token => $token,
                   di => $token->{di}, index => $token->{index},
                   ns => HTMLNS,
                   local_name => 'meta',
