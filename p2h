  } elsif ($Input =~ /\G([\'])/gcs) {
  
              push @$Errors, {type => 'bare hcro', level => 'm',
                              di => $DI, index => $Offset + (pos $Input) - 1};
            
  push @{$Attr->{q<value>}}, [$Temp, $DI, $TempIndex];
  $State = A_ATTR_VALUE__QUOTED__STATE;
